{
  "name": "bison-simulation-webapp",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "tsx watch server/index.ts",
    "server:python": "cd python_backend && python server.py",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "keywords": [
    "bison",
    "simulation",
    "ecology",
    "typescript",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "description": "Interactive web application for simulating bison population dynamics across landscapes",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-scale-chromatic": "^3.1.0",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.2",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "cors": "^2.8.6",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.1.0",
    "express": "^5.2.1",
    "geotiff": "^2.1.4-beta.1",
    "proj4": "^2.20.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsx": "^4.21.0"
  }
}
